	<!-- Composite Bow -->
	<!-- /ce data/ -->
	<Operation Class="CombatExtended.PatchOperationMakeGunCECompatible">
		<defName>MedTimes_Ranged_Composite_Nan</defName>
		<statBases>
			<SightsEfficiency>0.75</SightsEfficiency>
			<ShotSpread>1</ShotSpread>
			<SwayFactor>2</SwayFactor>
			<Bulk>3.00</Bulk>
			<RangedWeapon_Cooldown>1</RangedWeapon_Cooldown>
		</statBases>
		<Properties>
			<verbClass>CombatExtended.Verb_ShootCE</verbClass>
			<hasStandardCommand>true</hasStandardCommand>
			<defaultProjectile>Medieval_Arrow_Composite</defaultProjectile>
			<warmupTime>1</warmupTime>
			<range>29</range>
			<soundCast>Bow_Small</soundCast>
		</Properties>
        <AmmoUser>
          <ammoSet>AmmoSet_Arrow</ammoSet>
        </AmmoUser>
        <FireModes>
          <aiAimMode>AimedShot</aiAimMode>
        </FireModes>
	</Operation>
	<!-- /tools/ -->
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="MedTimes_Ranged_Composite_Nan"]</xpath>
		<value>
			<tools>
				<li Class="CombatExtended.ToolCE">
					<label>limb</label>
					<capacities>
						<li>Blunt</li>
					</capacities>
					<power>8</power>
					<cooldownTime>1.6</cooldownTime>
					<armorPenetration>0.102</armorPenetration>
					<linkedBodyPartsGroup>Base</linkedBodyPartsGroup>
				</li>
				<li Class="CombatExtended.ToolCE">
					<label>nock</label>
					<capacities>
						<li>Poke</li>
					</capacities>
					<power>8</power>
					<cooldownTime>1.6</cooldownTime>
					<armorPenetration>0.075</armorPenetration>
					<linkedBodyPartsGroup>Point</linkedBodyPartsGroup>
				</li>
			</tools>
		</value>
	</Operation>
	<!-- /tags/ -->
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="MedTimes_Ranged_Composite_Nan"]</xpath>
		<value>
			<weaponTags>
				<li>CE_Bow</li>
			</weaponTags>
		</value>
	</Operation>
